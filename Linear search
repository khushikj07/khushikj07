#include<iostream>
using namespace std;
int linearsearch(int *arr, int n, int val)
{
   for(int i=0;i<n;i++)
   {
     if(arr[i]==val){
        return i;
      }
   }
return -1;
}
int main()
{
   int n;
   cin>>n;
   int *arr=new int[n];
   for(int i=0;i<n;i++)
   {
      cin>>arr[i];
   }
int val;
cin>>val;
cout<<linearsearch(arr,n,val);
return 0;
}
 

